FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      B0;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -2 0 0 -1 0];


internalField   uniform (0.666 0 0);

boundaryField
{
	symPlane
	{
	type 			symmetry;
	}
	
    ".*"
    {
        type            calculated;
        value           $internalField;
    }
}

